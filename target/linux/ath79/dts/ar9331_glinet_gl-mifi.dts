/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar9331.dtsi"

/ {
	model = "GL.iNet GL-MiFi";
	compatible = "glinet,gl-mifi", "qca,ar9331";

	aliases {
		serial0 = &uart;
		led-boot = &led_net;
		led-failsafe = &led_net;
		led-running = &led_net;
		led-upgrade = &led_net;
	};

	leds {
		compatible = "gpio-leds";

		wan {
			label = "gl-mifi:green:wan";
			gpios = <&gpio 27 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		lan {
			label = "gl-mifi:green:lan";
			gpios = <&gpio 16 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		wlan {
			label = "gl-mifi:green:wlan";
			gpios = <&gpio 1 GPIO_ACTIVE_HIGH>;
			default-state = "off";
			linux,default-trigger = "phy0tpt";
		};

		led_net: net {
			label = "gl-mifi:green:net";
			gpios = <&gpio 0 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

        3gcontrol {
			label = "gl-mifi:green:3gcontrol";
			gpios = <&gpio 7 GPIO_ACTIVE_HIGH>;
 			default-state = "off";
		};
	};

	keys {
		compatible = "gpio-keys";

		button0 {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 11 GPIO_ACTIVE_HIGH>;
			debounce-interval = <60>;
		};
	};
};

&ref {
	clock-frequency = <25000000>;
};

&uart {
	status = "okay";
};

&gpio {
	status = "okay";
};

&usb {
	#address-cells = <1>;
	#size-cells = <0>;
	dr_mode = "host";
	status = "okay";

	hub_port: port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&usb_phy {
	status = "okay";
};

&spi {
	num-chipselects = <1>;
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		spi-max-frequency = <104000000>;
		reg = <0>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot:	partition@0 {
				label = "u-boot";
				reg = <0x000000 0x040000>;
				read-only;
			};

			partition@40000 {
				label = "u-boot-env";
				reg = <0x040000 0x010000>;
				read-only;
			};

			partition@50000 {
				label = "firmware";
				reg = <0x050000 0x0fa0000>;
			};

			art: partition@ff0000 {
				label = "art";
				reg = <0xff0000 0x010000>;
				read-only;
			};
		};
	};
};


&eth0 {
	status = "okay";

	mtd-mac-address = <&art 0x0000>;
	gmac-config {
		device = <&gmac>;

		switch-phy-addr-swap = <0>;
		switch-phy-swap = <0>;
	};
};

&eth1 {
	status = "okay";

	mtd-mac-address = <&art 0x0000>;
};

&wmac {
	status = "okay";

	mtd-cal-data = <&art 0x1000>;
};
