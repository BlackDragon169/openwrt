/dts-v1/;

#include "qca9563_tplink_archer-x7-v5.dtsi"

/ {
	compatible = "tplink,archer-c7-v5", "qca,qca9563";
	model = "TP-Link Archer C7 v5";
};

&mtdparts {
	factory-uboot@0 {
		label = "factory-uboot";
		reg = <0x000000 0x020000>;
		read-only;
	};

	uboot: u-boot@20000 {
		label = "u-boot";
		reg = <0x020000 0x020000>;
		read-only;
	};

	art: art@50000 {
		label = "art";
		reg = <0x050000 0x010000>;
		read-only;
	};

	info: info@60000 {
		label = "info";
		reg = <0x060000 0x010000>;
		read-only;
	};

	firmware@c0000 {
		label = "firmware";
		reg = <0xc0000 0xf00000>;
	};
};
